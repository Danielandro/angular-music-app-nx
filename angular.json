{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "music-app-nx": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/music-app-nx",
      "sourceRoot": "apps/music-app-nx/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/music-app-nx",
            "index": "apps/music-app-nx/src/index.html",
            "main": "apps/music-app-nx/src/main.ts",
            "polyfills": "apps/music-app-nx/src/polyfills.ts",
            "tsConfig": "apps/music-app-nx/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/music-app-nx/src/favicon.ico",
              "apps/music-app-nx/src/assets"
            ],
            "styles": [
              "apps/music-app-nx/src/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/assets/scss/modules/"
              ]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/music-app-nx/src/environments/environment.ts",
                  "with": "apps/music-app-nx/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "music-app-nx:build",
            "proxyConfig": "apps/music-app-nx/src/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "music-app-nx:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "music-app-nx:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/music-app-nx/src/test.ts",
            "polyfills": "apps/music-app-nx/src/polyfills.ts",
            "tsConfig": "apps/music-app-nx/tsconfig.spec.json",
            "karmaConfig": "apps/music-app-nx/karma.conf.js",
            "assets": [
              "apps/music-app-nx/src/favicon.ico",
              "apps/music-app-nx/src/assets"
            ],
            "styles": [
              "apps/music-app-nx/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/music-app-nx/tsconfig.app.json",
              "apps/music-app-nx/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ],
            "fix": true
          }
        }
      }
    },
    "music-app-nx-e2e": {
      "projectType": "application",
      "root": "apps/music-app-nx-e2e",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/music-app-nx-e2e/protractor.conf.js",
            "devServerTarget": "music-app-nx:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "music-app-nx:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/music-app-nx-e2e/tsconfig.json",
            "exclude": [
              "**/node_modules/**"
            ],
            "fix": true
          }
        }
      }
    }
  },
  "defaultProject": "music-app-nx",
  "cli": {
    "analytics": false,
    "defaultCollection": "@ngxs/schematics"
  }
}
