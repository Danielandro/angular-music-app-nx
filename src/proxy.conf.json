{
  "/api/*": {
    "target": "https://api.deezer.com",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true,
    "pathRewrite": {
      "^/api": ""
    }
  },
  "/auth/*": {
    "target": "https://connect.deezer.com/oauth/auth.php?",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true,
    "pathRewrite": {
      "^/auth": ""
    }
  }
}
